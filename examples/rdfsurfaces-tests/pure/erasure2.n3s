@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

# Erasure test

# ~(B . ~(C . ~(B . D))) = B -> C . ~D
# Make a B
:B a :B2 .
() log:onNegativeSurface {
    :B a :B2 .

    () log:onNegativeSurface {
        # C
        :C a :C2 .

        () log:onNegativeSurface {
            :B a :B2 .
            :D a :D2 .
        } .
    } .
} .

# Verbose
(_:S _:O) log:onNegativeSurface {
    _:S a _:O .
    () log:onNegativeAnswerSurface {
        _:S a _:O .
    } .
} .

(_:S _:O) log:onNegativeSurface {
    () log:onNegativeSurface {
        _:S a _:O .
    } .
    () log:onNegativeAnswerSurface {
        () log:onNegativeSurface {
            _:S a _:O .
        } .
    } .
} .

# Test
() log:onNegativeSurface {
    :C a :C2 .

    () log:onNegativeSurface {
        :D a :D2 .
    } .

    () log:onNegativeSurface {
        :test :is true .
    } .
} .

() log:onNegativeSurface { :test :is true. () log:onNegativeAnswerSurface { :test :is true } } .
