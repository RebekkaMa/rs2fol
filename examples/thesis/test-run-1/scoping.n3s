@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

_:x a :Cat .
:Minka a :Cat .

(_:x) log:onNegativeSurface {
    _:x a :Cat .
    () log:onNegativeSurface {
        _:x :says "meow" .
    } .
} .

() log:onNegativeSurface {
    _:x :says "meow" .
    :Minka :says "meow" .
    () log:onNegativeSurface {
        :test :is true .
    } .
} .

() log:onNegativeSurface { :test :is true. () log:onNegativeAnswerSurface { :test :is true } } .
